#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main()
{

  int n,m;
  cin >> n >> m;
  int but;
  int arr[500];
  int c;
    for(int i=0;i<n;++i){

    cin >> but;

    for(int g=0;g<but;++g){
        cin >> c;
        arr[c] = 1;

    }

  }


bool x =0;




         for(int g=1;g<=m;++g){

          if(arr[g] !=1) x = 1;

         }




if(x) cout << "NO";
else cout << "YES";



    return 0;
}
