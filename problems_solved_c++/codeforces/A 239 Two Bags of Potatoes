#include <bits/stdc++.h>
#define ll long long
using namespace std;
ll y,k,n;
ll sum  = 0;
ll temp=0;
int f = 1;
int main()
{
    scanf("%d %d %d",&y,&k,&n);

    if(y >= n) cout << -1;
    else
    {
        for(ll j=1;j<=(n/k);++j)
        {
               if(j*k > y){

              cout << (j*k - y) << " ";
                f=0;

            }

        }
        if(f) cout << "-1";

    }


    return 0;
}
