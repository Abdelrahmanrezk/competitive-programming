#include <iostream>
#include <bits/stdc++.h>
using namespace std;

bool contest(int n){

    bool x = 0;
    string s;
    int b,a;
    for(int i=0;i<n;++i){
        cin >> s >> b >> a;
        if(b >=2400 && a > b) {

            x = 1;
            break;
        }

    }


 return x;
}
int n;
int main()
{
        cin >> n;
        if(contest(n)) cout << "YES";
        else cout << "NO";

    return 0;
}
