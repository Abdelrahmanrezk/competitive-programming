#include <iostream>
#include <bits/stdc++.h>
using namespace std;




int main()
{


    int n,k;
    int arr[100][100] = {{0}};
    scanf("%d %d", &n,&k);

        int x = n-1;
        for(int i=0;i<n;++i)
        {
            for(int g=0;g < n;++g)
            {
                if(g == x)
                {
                printf("%d ", k);


                }else{

                    printf("%d ", arr[i][g]);

                }
            }
            printf("\n");
            --x;
        }

    return 0;
}
