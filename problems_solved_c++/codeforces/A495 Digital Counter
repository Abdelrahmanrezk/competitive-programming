#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    string s;
    cin >> s;
    int one = 0,two=0;


        if(s[0] == '2' || s[0] == '6' || s[0] == '9' || s[0] == '0') one = 2;
        else if(s[0]  == '3' || s[0]  == '4') one =3;
        else if(s[0]  == '1') one = 7;
        else if(s[0]  == '5') one = 4;
        else if(s[0]  == '7') one = 5;
        else one = 1;


  if(s[1]  == '2' || s[1]  == '6' || s[1]  == '9' || s[1]  == '0') two = 2;
        else if(s[1] == '3' || s[1] == '4') two =3;
        else if(s[1]  == '1') two =7;
        else if(s[1]  == '5') two = 4;
        else if(s[1]  == '7') two = 5;
        else two = 1;


    cout << (one * two);
    return 0;
}
