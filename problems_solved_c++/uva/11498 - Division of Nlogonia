#include <iostream>

using namespace std;

int main()
{

    int t,m,n,x,y;
    while(scanf("%d",&t), (t))
    {
       scanf("%d %d",&m,&n);
        while(t--)
        {
            scanf("%d %d",&x,&y);

            if(x == m || y == n) printf("divisa\n");
            else if(x > m && y > n) printf("NE\n");
            else if(x < m && y < n) printf("SO\n");
            else if(x > m && y < n) printf("SE\n");
            else if(x < m && y > n) printf("NO\n");
        }

    }
    return 0;
}
