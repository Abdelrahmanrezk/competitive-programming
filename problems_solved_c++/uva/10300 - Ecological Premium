#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n,f,x,y,z,sum=0;
    while(scanf("%d",&n)  != EOF)
    {
        while(n--)
        {
            sum=0;
            scanf("%d",&f);
            while(f--)
            {
                scanf("%d %d %d",&x,&y,&z);

                sum += x * z;
            }
            printf("%d\n",sum);
        }
    }
    return 0;
}
