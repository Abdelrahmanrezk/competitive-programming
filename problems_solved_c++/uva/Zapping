#include <bits/stdc++.h>

using namespace std;

int main()
{

    int a,b,sum1=0,sum2=0;
    while(scanf("%d %d",&a,&b) && (a != -1 || b != -1))
    {
	if(a>b) swap(a,b)
        sum1=0,sum2=0;
        sum1 = abs(a-b);
        sum2 =a;
        ++sum2;
        sum2 += (99-b);
        int mins = min(sum1,sum2);
        printf("%d\n",mins);

    }
    return 0;
}
